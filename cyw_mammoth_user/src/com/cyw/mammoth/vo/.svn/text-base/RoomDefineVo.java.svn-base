package com.cyw.mammoth.vo;

import org.apache.commons.lang.builder.ToStringBuilder;

import com.cyw.mammoth.bean.Rooms;

public class RoomDefineVo {
	/**
	 * 房间实体
	 */
	private Rooms room ;
	/**
	 * 建筑物名称
	 */
	private String buildingName ;
	/**
	 * 房型名称
	 */
	private String roomTypeName ;
	/**
	 * 房型代码
	 */
	private String roomTypeCode ;
	/**
	 * 编辑状态   'u' 修改   'a' 新增 
	 */
	private String editFlag ;
	/**
	 * 建筑的最小楼层
	 */
	private Integer buildMinNum ;
	/**
	 * 建筑的最大楼层
	 */
	private Integer buildMaxNum ;
	/**
	 * 房间特征名称
	 */
	private String roomCharaterNames ;
	
	public Rooms getRoom() {
		return room;
	}


	public void setRoom(Rooms room) {
		this.room = room;
	}


	public String getBuildingName() {
		return buildingName;
	}


	public void setBuildingName(String buildingName) {
		this.buildingName = buildingName;
	}


	public String getRoomTypeName() {
		return roomTypeName;
	}


	public void setRoomTypeName(String roomTypeName) {
		this.roomTypeName = roomTypeName;
	}

	public String getEditFlag() {
		return editFlag;
	}


	public void setEditFlag(String editFlag) {
		this.editFlag = editFlag;
	}


	public String getRoomTypeCode() {
		return roomTypeCode;
	}


	public void setRoomTypeCode(String roomTypeCode) {
		this.roomTypeCode = roomTypeCode;
	}


	public Integer getBuildMinNum() {
		return buildMinNum;
	}


	public void setBuildMinNum(Integer buildMinNum) {
		this.buildMinNum = buildMinNum;
	}


	public Integer getBuildMaxNum() {
		return buildMaxNum;
	}


	public void setBuildMaxNum(Integer buildMaxNum) {
		this.buildMaxNum = buildMaxNum;
	}


	public String getRoomCharaterNames() {
		return roomCharaterNames;
	}


	public void setRoomCharaterNames(String roomCharaterNames) {
		this.roomCharaterNames = roomCharaterNames;
	}


	@Override
	public String toString() {
		return ToStringBuilder.reflectionToString(this);
	}
	
	

}
